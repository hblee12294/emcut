{
  "extends": "@emcut/typescript-config/react-library.json",
  "compilerOptions": {
    "outDir": "dist",
    "module": "esnext",
    "moduleResolution": "bundler",
    "isolatedModules": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx",],
  "exclude": ["node_modules", "dist"]
}
